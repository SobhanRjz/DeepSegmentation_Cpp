        }
      ],
      "preLaunchTask": "Build Unified Batch Processor (Release)",
      "miDebuggerPath": "/usr/bin/gdb"
    },
    {
      "name": "üß™ Test OpenCV (Release)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/release/test_opencv",
      "cwd": "${workspaceFolder}",
      "environment": [
        { 
          "name": "LD_LIBRARY_PATH", 
          "value": "${workspaceFolder}/libs/opencv_install/lib:/usr/local/lib" 
        }
      ],
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "stopAtEntry": false,
      "preLaunchTask": "Build Test OpenCV (Release)",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "üß™ Run All Tests (Debug)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/tests/build/yolo_tests",
      "args": ["--gtest_break_on_failure", "--gtest_output=verbose"],
      "cwd": "${workspaceFolder}",
      "environment": [
        { 
          "name": "LD_LIBRARY_PATH", 
          "value": "${workspaceFolder}/libs/opencv_install/lib:/usr/local/lib" 
        }
      ],
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "stopAtEntry": false,
      "preLaunchTask": "Build Tests (Debug)",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set print elements to unlimited",
          "text": "set print elements 0",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "üìä CSV Comparison Test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/tests/build/yolo_tests",
      "args": ["--gtest_filter=CSVComparisonTest.*", "--gtest_output=verbose"],
      "cwd": "${workspaceFolder}",
      "environment": [
        { 
          "name": "LD_LIBRARY_PATH", 
          "value": "${workspaceFolder}/libs/opencv_install/lib:/usr/local/lib" 
        }
      ],
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "stopAtEntry": false,
      "preLaunchTask": "Build Tests (Debug)",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "üèÉ Run Tests (Release)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/tests/build/yolo_tests",
      "args": ["--gtest_output=verbose"],
      "cwd": "${workspaceFolder}",
      "environment": [
        { 
          "name": "LD_LIBRARY_PATH", 
          "value": "${workspaceFolder}/libs/opencv_install/lib:/usr/local/lib" 
        }
      ],
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "stopAtEntry": false,
      "preLaunchTask": "Build Tests (Release)"
    }
  ],
  "inputs": [
    {
      "id": "testFilter",
      "description": "Enter test filter (e.g., CSVComparisonTest.*, PostprocessingTest.*)",
      "default": "*",
      "type": "promptString"
    },
    {
      "id": "imagePath",
      "description": "Enter path to image file",
      "default": "${workspaceFolder}/test_output.jpg",
      "type": "promptString"
    },
    {
      "id": "modelPath",
      "description": "Enter path to .pt model file",
      "default": "${workspaceFolder}/YoloModel/yolov8n.pt",
      "type": "promptString"
    },
    {
      "id": "pythonArgs",
      "description": "Enter Python script arguments",
      "default": "",
      "type": "promptString"
    }
  ]
}
